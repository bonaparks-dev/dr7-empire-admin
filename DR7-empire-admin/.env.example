# Frontend Environment Variables (Vite - exposed to browser)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_ADMIN_UI_TOKEN=your_admin_ui_token_here

# Backend Environment Variables (Netlify Functions - server-only)
# Add these in Netlify dashboard under Site settings > Environment variables
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
ADMIN_API_TOKEN=your_admin_api_token_here

# CallMeBot Configuration
# Add your admin WhatsApp number in international format (e.g., 393457905205)
CALLMEBOT_ADMIN_PHONE=your_admin_whatsapp_number
CALLMEBOT_API_KEY=your_callmebot_api_key
